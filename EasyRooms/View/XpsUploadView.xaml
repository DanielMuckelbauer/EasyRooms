<UserControl x:Class="EasyRooms.View.XpsUploadView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EasyRooms.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <local:XpsUploadViewModel/>
    </UserControl.DataContext>
    <Grid Margin="0,0,0,-63">
        <TextBox AcceptsReturn="True" Text="{Binding RoomsString}" Margin="40,23,671,101"></TextBox>
        <Button Click="SelectFile_Click" Margin="257,22,360,389" RenderTransformOrigin="0.126,5.296">Datei Auswählen</Button>
        <!--<Button Command="{Binding CalculateCommand}" Margin="257,100,360,306">Berechnen</Button>-->
    </Grid>
</UserControl>
