             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EasyRooms.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <local:XpsUploadViewModel/>
    </UserControl.DataContext>
    <Grid>
        <TextBox Text="{Binding RoomsString}" Margin="144,30,567,341"></TextBox>
        <Button Click="SelectFile_Click" Margin="122,152,495,259">Datei Auswählen</Button>
    </Grid>
</UserControl>
